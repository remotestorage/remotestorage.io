@import '_settings'

global-reset()
base()

footer_height = 50px

// grid()
// column-width: 50
// gutter-width: 30
// columns: 12
// total-width: 960px

*
  -webkit-font-smoothing: auto

html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p,
blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, img,
ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, dl, dt, dd, ol,
ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr,
th, td
  border: none

.container
  max-width: 960px
  margin: 0 auto

body
  color: #333
  bg "data:image/png;base64," repeat scroll 0% 0% #fff
  text-align: center

header
  bg "data:image/png;base64," repeat scroll 0% 0% #43413E
  float: left
  width: 100%
  padding: 15px 0

  .logo
    column(3)
    text-align: left
    h1
      a
        display: block
        text-indent: -100000px
        size: 260px 40px
        bg: "/img/remotestorage-wide.svg"
        background-size: auto 40px
        // background: #eee

  nav
    column(9)
    text-align: left
    // background: #ccc
    ul
      float: right
      margin-top: 5px
      li
        display: inline-block
        font-size: 14px
        margin: 0 5px
        // text-transform: uppercase
        a
          display: block
          text-decoration: none
          color: #eee
          line-height: 30px
          padding: 0 10px
          border-radius: 5px
          text-shadow: 0 1px 1px #000
          transition: all 0.2s linear;
          &:hover, &.active
            color: #fff
            background-color: #333

.hero
  bg "data:image/png;base64," repeat scroll 0% 0% #eee
  float: left
  width: 100%
  section.content
    h2
      font-size: 28px
      margin 0 0 10px 0
      // text-shadow: 0 1px 3px #fff
    p
      &.claim
        font-size: 20px
        margin 0 0 28px 0
        // text-shadow: 0 1px 3px #fff
      &.specs
        a
          text-decoration: none
          color: #333
          border-bottom: 1px solid #aaa
          transition: all 0.2s linear;
          &:hover
            border-bottom: 1px solid #666
            cursor: help
      &:last-child
        margin-bottom: 0

section.content
  column(12)
  text-align: left
  padding: 50px 0
  h2
    font-size: 24px
    margin: 0 0 24px 0
    font-weight: bold
  a
    link(#d92b00)
  h3
    font-weight: bold
    margin-bottom: 14px
    text-transform: uppercase
  h4
    font-weight: bold
    margin-bottom: 10px
    line-height: 1.4em
  p
    line-height: 1.4em
    font-size: 14px
    margin-bottom: 20px

.benefits
  row(12)
  float: left
  padding-bottom: 30px
  section
    column(4,12)
    p
      margin: 0

section.content .get-storage
  row(12)
  .providers, .selfhost
    column(6)
    &> p
      margin-bottom: 20px
      color: #888
    li
      margin: 0 0 20px 0

section.content .integrate
  img.widget
    float: right
    margin: 0 0 20px 20px

.unfinished
  column(12)
  padding-top: 50px
  margin: 0
  p
    notice()

footer
  bg "data:image/png;base64," repeat scroll 0% 0% #43413E
  float: left
  width: 100%
  // padding: 20px 0
  font-size: 12px
  color: #ccc
  text-shadow: 0 1px 1px #000
  p
    line-height: footer_height

// Sticky footer
html
  height: 100%
body
  height: 100%
#wrapper
  min-height: 100%
#wrapper-content
  padding-bottom: footer_height
.space
  clear: both
  height: 30px // ugly hack
footer
  margin-top: 0 - footer_height
  height: footer_height
